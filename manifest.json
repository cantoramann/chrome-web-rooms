{
    "manifest_version": 3,
    "name": "Chat on websites!",
    "version": "1.0",
    "description": "Chat on websites!",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}